extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug
include ../blocks/sLive/_sLive.pug
include ../blocks/sAbout/_sAbout.pug
include ../blocks/sSpeakers/_sSpeakers.pug
include ../blocks/sExponents/_sExponents.pug
include ../blocks/sPartners/_sPartners.pug
include ../blocks/sMembers/_sMembers.pug
include ../blocks/sStream/_sStream.pug
include ../blocks/sQuestions/_sQuestions.pug

block variables
	- let  mainPage= null 
	- let  bg= null

block title
	- let  title= 'LIVE'
	title= title
block pagecontent
	.main-wrapper
		+mob-mnu()
		+top-nav()
		+b.page-head.section

			.section__inner.container-fluid 
				+b.link-back(href="#")
					+icon("arrow-back")
				h1= title
		+sStream
		.container-fluid
			+b.main-row.row.row--lg 
				+e.speakers-col.col-xl-6
					.section-title
						h2 Спикеры
					+sSpeakers12
				+e.questions-col.col-xl-6
					.section-title
						a.btn.btn-primary.link-modal(href="#modal-ask-question") Задать Вопрос
					+sQuestions
		+footer

	